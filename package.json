  "scripts": {
    "postinstall": "node scripts/patch-angular-build.js",
    "docs:sync-openapi": "node tools/docs/sync-openapi.js",
    "docs:coverage": "node tools/doc-coverage/doc-coverage.js",
    "docs:check": "node tools/docs/sync-openapi.js && nx run docs:check",
    "docs:dev": "node tools/docs/sync-openapi.js && npx astro dev --root docs-site",
    "docs:build": "node tools/docs/sync-openapi.js && npx astro build --root docs-site",
    "docs:preview": "npx astro preview --root docs-site",
    "test:ci": "npx nx run-many -t test --configuration=ci --projects=backend,web,infra",
    "ci:verify": "NX_CLOUD=false NX_DAEMON=false NX_ISOLATE_PLUGINS=false npx nx run-many -t lint test build && NX_CLOUD=false NX_DAEMON=false NX_ISOLATE_PLUGINS=false npm run docs:check && AWS_ACCOUNT_ID=$AWS_ACCOUNT_ID npx cdk synth -c rootDomainName=$ROOT_DOMAIN -c hostedZoneId=$HOSTED_ZONE_ID -c apiDomainName=$API_DOMAIN -c webDomainName=$WEB_DOMAIN -c sesFromAddress=$SES_FROM_ADDRESS -c sesMailFromDomain=$SES_MAIL_FROM_DOMAIN -c enableLambdaVpc=$ENABLE_LAMBDA_VPC"
  },
  "private": true,
  "devDependencies": {
