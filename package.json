    "@eslint/js": "^9.8.0",
    "@nx/angular": "^22.4.1",
    "@nx/esbuild": "22.4.1",
    "@nx/eslint": "22.4.1",
    "@nx/eslint-plugin": "22.4.1",
    "@nx/js": "22.4.1",
    "@nx/node": "^22.4.1",
    "@nx/web": "22.4.1",
    "@schematics/angular": "~21.1.1",
    "@swc-node/register": "~1.11.1",
    "@swc/core": "~1.15.10",
    "@swc/helpers": "~0.5.18",
    "@types/aws-lambda": "^8.10.160",
    "@types/node": "25.0.10",
    "@typescript-eslint/utils": "^8.53.1",
    "astro": "^5.16.15",
    "angular-eslint": "^21.0.1",
    "aws-cdk": "^2.1103.0",
    "aws-cdk-lib": "^2.236.0",
    "constructs": "^10.4.5",
