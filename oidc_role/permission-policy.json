{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "CDKBootstrap",
            "Effect": "Allow",
            "Action": [
                "cloudformation:*",
                "iam:GetRole",
                "iam:CreateRole",
                "iam:DeleteRole",
                "iam:*RolePolicy",
                "iam:*ttachRolePolicy",
                "iam:PassRole",
                "iam:*agRole",
                "iam:ListRole*",
                "s3:*Bucket*",
                "s3:*Object*",
                "s3:ListBucket",
                "s3:GetBucketLocation",
                "ssm:*Parameter*",
                "ecr:*Repository*",
                "ecr:*Tag*",
                "ecr:*Policy*",
                "sts:AssumeRole"
            ],
            "Resource": [
                "arn:aws:cloudformation:*:$ACCOUNT_ID:stack/Sdg19*Stack/*",
                "arn:aws:cloudformation:*:$ACCOUNT_ID:stack/CDKToolkit/*",
                "arn:aws:iam::$ACCOUNT_ID:role/Sdg19*Stack-*",
                "arn:aws:iam::$ACCOUNT_ID:role/cdk-*",
                "arn:aws:s3:::cdk-*",
                "arn:aws:s3:::cdk-*/*",
                "arn:aws:s3:::sdg19*stack-*",
                "arn:aws:s3:::sdg19*stack-*/*",
                "arn:aws:ssm:*:$ACCOUNT_ID:parameter/cdk-bootstrap/*",
                "arn:aws:ecr:*:$ACCOUNT_ID:repository/cdk-*"
            ]
        },
        {
            "Sid": "AWSServices",
            "Effect": "Allow",
            "Action": [
                "lambda:*",
                "dynamodb:*Table*",
                "dynamodb:*Stream*",
                "dynamodb:*Tag*",
                "apigateway:*",
                "cognito-idp:*UserPool*",
                "cognito-idp:*Tag*",
                "sns:*Topic*",
                "sns:*Tag*",
                "sns:Subscribe",
                "sns:Unsubscribe",
                "sqs:*Queue*",
                "sqs:*Tag*",
                "kinesis:*Stream*",
                "kinesis:*Tag*",
                "firehose:*DeliveryStream*",
                "firehose:*Tag*",
                "ses:*EmailIdentity*",
                "ses:*EmailTemplate*",
                "ses:*Dkim*",
                "ses:*MailFrom*",
                "route53:*ResourceRecordSets",
                "route53:GetHostedZone",
                "route53:GetChange",
                "acm:*Certificate*",
                "acm:*Tag*",
                "wafv2:*WebACL*",
                "wafv2:*Tag*",
                "logs:*LogGroup*",
                "logs:*Tag*",
                "logs:*ResourcePolicy*",
                "logs:*RetentionPolicy*",
                "cloudwatch:*Alarm*",
                "cloudwatch:*Tag*",
                "cloudtrail:*Trail*",
                "cloudtrail:*Tag*",
                "cloudtrail:*Logging*",
                "kms:*Key*",
                "kms:*Alias*",
                "kms:*Tag*"
            ],
            "Resource": [
                "arn:aws:lambda:*:$ACCOUNT_ID:function:Sdg19*Stack-*",
                "arn:aws:lambda:*:$ACCOUNT_ID:event-source-mapping:*",
                "arn:aws:dynamodb:*:$ACCOUNT_ID:table/Sdg19*Stack-*",
                "arn:aws:dynamodb:*:$ACCOUNT_ID:table/Sdg19*Stack-*/stream/*",
                "arn:aws:apigateway:*::/apis*",
                "arn:aws:cognito-idp:*:$ACCOUNT_ID:userpool/*",
                "arn:aws:sns:*:$ACCOUNT_ID:Sdg19*Stack-*",
                "arn:aws:sqs:*:$ACCOUNT_ID:Sdg19*Stack-*",
                "arn:aws:kinesis:*:$ACCOUNT_ID:stream/Sdg19*Stack-*",
                "arn:aws:firehose:*:$ACCOUNT_ID:deliverystream/Sdg19*Stack-*",
                "arn:aws:route53:::hostedzone/*",
                "arn:aws:route53:::change/*",
                "arn:aws:wafv2:*:$ACCOUNT_ID:global/webacl/Sdg19*Stack-*/*",
                "arn:aws:logs:*:$ACCOUNT_ID:log-group:/aws/lambda/Sdg19*Stack-*",
                "arn:aws:logs:*:$ACCOUNT_ID:log-group:/aws/apigateway/Sdg19*Stack-*",
                "arn:aws:logs:*:$ACCOUNT_ID:resource-policy:*",
                "arn:aws:cloudwatch:*:$ACCOUNT_ID:alarm:Sdg19*Stack-*",
                "arn:aws:cloudtrail:*:$ACCOUNT_ID:trail/Sdg19*Stack-*",
                "arn:aws:kms:*:$ACCOUNT_ID:key/*",
                "arn:aws:kms:*:$ACCOUNT_ID:alias/Sdg19*",
                "*"
            ]
        },
        {
            "Sid": "VPCOperations",
            "Effect": "Allow",
            "Action": [
                "ec2:*Vpc*",
                "ec2:*Subnet*",
                "ec2:*RouteTable*",
                "ec2:*Route",
                "ec2:*InternetGateway*",
                "ec2:*NatGateway*",
                "ec2:*Address*",
                "ec2:*SecurityGroup*",
                "ec2:*Tags*",
                "ec2:Describe*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "CloudFrontOperations",
            "Effect": "Allow",
            "Action": [
                "cloudfront:*Distribution*",
                "cloudfront:*OriginAccessControl*",
                "cloudfront:*CachePolicy*",
                "cloudfront:*ResponseHeadersPolicy*",
                "cloudfront:*Tag*",
                "cloudfront:CreateInvalidation",
                "cloudfront:List*"
            ],
            "Resource": "*"
        }
    ]
}