{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "CDKBootstrap",
      "Effect": "Allow",
      "Action": [
        "cloudformation:*",
        "iam:GetRole",
        "iam:CreateRole",
        "iam:DeleteRole",
        "iam:PutRolePolicy",
        "iam:GetRolePolicy",
        "iam:DeleteRolePolicy",
        "iam:AttachRolePolicy",
        "iam:DetachRolePolicy",
        "iam:ListRolePolicies",
        "iam:ListAttachedRolePolicies",
        "iam:PassRole",
        "iam:TagRole",
        "iam:UntagRole",
        "iam:ListRoleTags",
        "s3:*Bucket*",
        "s3:*Object*",
        "s3:ListBucket",
        "s3:GetBucketLocation",
        "ssm:*Parameter*",
        "ecr:*Repository*",
        "ecr:*Tag*",
        "ecr:*Policy*",
        "sts:AssumeRole"
      ],
      "Resource": [
        "arn:aws:cloudformation:*:605134457500:stack/Sdg19*Stack/*",
        "arn:aws:cloudformation:*:605134457500:stack/CDKToolkit/*",
        "arn:aws:iam::605134457500:role/Sdg19*Stack-*",
        "arn:aws:iam::605134457500:role/cdk-*",
        "arn:aws:s3:::cdk-*",
        "arn:aws:s3:::cdk-*/*",
        "arn:aws:s3:::sdg19*stack-*",
        "arn:aws:s3:::sdg19*stack-*/*",
        "arn:aws:ssm:*:605134457500:parameter/cdk-bootstrap/*",
        "arn:aws:ecr:*:605134457500:repository/cdk-*"
      ]
    },
    {
      "Sid": "AWSServices",
      "Effect": "Allow",
      "Action": [
        "lambda:*",
        "dynamodb:*Table*",
        "dynamodb:*Stream*",
        "dynamodb:*Tag*",
        "apigateway:*",
        "cognito-idp:*UserPool*",
        "cognito-idp:*Tag*",
        "sns:*Topic*",
        "sns:*Tag*",
        "sns:Subscribe",
        "sns:Unsubscribe",
        "sqs:*Queue*",
        "sqs:*Tag*",
        "kinesis:*Stream*",
        "kinesis:*Tag*",
        "firehose:*DeliveryStream*",
        "firehose:*Tag*",
        "ses:*EmailIdentity*",
        "ses:*EmailTemplate*",
        "ses:*Dkim*",
        "ses:*MailFrom*",
        "route53:*ResourceRecordSets",
        "route53:GetHostedZone",
        "route53:GetChange",
        "acm:*Certificate*",
        "acm:*Tag*",
        "wafv2:*WebACL*",
        "wafv2:*Tag*",
        "logs:*LogGroup*",
        "logs:*Tag*",
        "logs:*ResourcePolicy*",
        "logs:*RetentionPolicy*",
        "cloudwatch:*Alarm*",
        "cloudwatch:*Tag*",
        "cloudtrail:*Trail*",
        "cloudtrail:*Tag*",
        "cloudtrail:*Logging*",
        "kms:*Key*",
        "kms:*Alias*",
        "kms:*Tag*"
      ],
      "Resource": [
        "arn:aws:lambda:*:605134457500:function:Sdg19*Stack-*",
        "arn:aws:lambda:*:605134457500:event-source-mapping:*",
        "arn:aws:dynamodb:*:605134457500:table/Sdg19*Stack-*",
        "arn:aws:dynamodb:*:605134457500:table/Sdg19*Stack-*/stream/*",
        "arn:aws:apigateway:*::/apis*",
        "arn:aws:cognito-idp:*:605134457500:userpool/*",
        "arn:aws:sns:*:605134457500:Sdg19*Stack-*",
        "arn:aws:sqs:*:605134457500:Sdg19*Stack-*",
        "arn:aws:kinesis:*:605134457500:stream/Sdg19*Stack-*",
        "arn:aws:firehose:*:605134457500:deliverystream/Sdg19*Stack-*",
        "arn:aws:route53:::hostedzone/*",
        "arn:aws:route53:::change/*",
        "arn:aws:wafv2:*:605134457500:global/webacl/Sdg19*Stack-*/*",
        "arn:aws:logs:*:605134457500:log-group:/aws/lambda/Sdg19*Stack-*",
        "arn:aws:logs:*:605134457500:log-group:/aws/apigateway/Sdg19*Stack-*",
        "arn:aws:logs:*:605134457500:resource-policy:*",
        "arn:aws:cloudwatch:*:605134457500:alarm:Sdg19*Stack-*",
        "arn:aws:cloudtrail:*:605134457500:trail/Sdg19*Stack-*",
        "arn:aws:kms:*:605134457500:key/*",
        "arn:aws:kms:*:605134457500:alias/Sdg19*",
        "*"
      ]
    },
    {
      "Sid": "VPCOperations",
      "Effect": "Allow",
      "Action": [
        "ec2:*Vpc*",
        "ec2:*Subnet*",
        "ec2:*RouteTable*",
        "ec2:*Route",
        "ec2:*InternetGateway*",
        "ec2:*NatGateway*",
        "ec2:*Address*",
        "ec2:*SecurityGroup*",
        "ec2:*Tags*",
        "ec2:Describe*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "CloudFrontOperations",
      "Effect": "Allow",
      "Action": [
        "cloudfront:*Distribution*",
        "cloudfront:*OriginAccessControl*",
        "cloudfront:*CachePolicy*",
        "cloudfront:*ResponseHeadersPolicy*",
        "cloudfront:*Tag*",
        "cloudfront:CreateInvalidation",
        "cloudfront:List*"
      ],
      "Resource": "*"
    }
  ]
}
